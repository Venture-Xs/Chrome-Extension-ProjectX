{


  "manifest_version": 3,
  "name": "Chrome Extension",
  "version": "1.0.0",
  "description": "Chrome Extension to help students in their studies and research",

    "permissions": [
        "scripting",
        "activeTab"
    ],

    "action": {
        "default_popup": "index.html"
        
    },

    "host_permissions": [
        "http://*/*",
        "https://*/*"
    ]
}